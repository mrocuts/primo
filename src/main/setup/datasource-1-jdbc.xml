<?xml version="1.0" encoding="UTF-8"?>
<jdbc-data-source xmlns="http://xmlns.oracle.com/weblogic/jdbc-data-source">
  <name>jdbc/dbprimo</name>
  <jdbc-driver-params>
    <url>jdbc:postgresql://localhost:5432/db_primos</url>
    <driver-name>org.postgresql.Driver</driver-name>
    <properties>
      <property>
        <name>user</name>
        <value>postgres</value>
      </property>
      <property>
        <name>password</name>
        <value>admin</value>
      </property>
    </properties>
  </jdbc-driver-params>
  <jdbc-data-source-params>
    <jndi-name>jdbc/dbprimo</jndi-name>
  </jdbc-data-source-params>
</jdbc-data-source>
